  procedures :
   -
     name : Initialize
     procedure_id : 1900256512960323071
     type_code : 14
     code : |1-
      // Summary: Used to switch the window to addition or modification mode
      // Syntax:
      //Initialize ()
      //
      // Parameters:
      //	None
      
      //	bInAddition (boolean): <specify the role of bInAddition>
      Procédure Initialize()
      
      // Hide the keyboard
      SIPVisible(False)
      
      if gnID = 0 then
      	HReset(Configuration)
      	STC_ActionTitle = <§$0006§>
      	
      	// Reset the file
      	HReset(Configuration)
      	
      	// Default values
      	EDT_LastName = ""
      	EDT_ServerName = ""
      	EDT_ConnectionTimeout = 20
      	EDT_Port = 21
      	// Passive mode by default
      	CBOX_PASSIVE[1] = True
      	CBOX_AUTENTH[1] = False
      	EDT_USERNAME = ""
      	EDT_PWD = ""
      	
      	// Caption of the button
      	BTN_Add..Caption = <§$0002§>
      	// Passive mode by default
      	CBOX_PASSIVE = True
      else
      	STC_ActionTitle = <§$0007§>
      	
      	// Search for the configuration
      	HReadSeekFirst(Configuration,ConfigurationID,gnID)
      	FileToScreen(WIN_EditConfiguration_Tablet,Configuration)
      	
      	// Caption of the button
      	BTN_Add..Caption = <§$0003§>
      	
      	if CBOX_AUTENTH = True then
      		GP_AUTHENT..State = Active
      	else
      		GP_AUTHENT..State = Grayed
      	end
      end
     type : 458752
   -
     name : TestConnection
     procedure_id : 1900256512960388607
     type_code : 14
     code : |1-
      
      // Summary: Try to connect with the current configuration
      // 
      Procédure TestConnection()
      
      stConfig is STConfiguration
      nConnectionID is int
      
      // Checks the FTP server
      if EDT_ServerName = "" then
      	Error(<§$0000§>)
      	
      	
      	<compile if ConfigurationType=Android>
      		ReturnToCapture(EDT_ServerName)
      	<else>
      		return
      	<end>
      end
      
      // Check the port number
      if EDT_Port = 0 then
      	Error(<§$0001§>)
      	
      	<compile if ConfigurationType=Android>
      		ReturnToCapture(EDT_Port)
      	<else>
      		return
      	<end>
      end
      
      // Retrieve the data
      ScreenToFile(MyWindow,Configuration)
      
      
      // Run the connection test
      NextTitle(<§$0013§>)
      HourGlass(True)
      
      // Configuration
      stConfig.sServerName = Configuration.ServerName
      stConfig.sUser = Configuration.User
      stConfig.sPassword = Configuration.Password
      stConfig.nPort = Configuration.Port
      stConfig.bPassive = Configuration.Passive
      stConfig.nTimeout = Configuration.TimeOut
      stConfig.nSecurity = Configuration.Security
      
      // Connection
      nConnectionID = FTPConnectConfiguration(stConfig)
       
       HourGlass(False)
      if nConnectionID = -1 then
      	Trace(<§@1cd27ae1000630230002§> + ErrorInfo)
      	Error(<§$0014§>,ErrorInfo)
      else
      	FTPDisconnect(gnConnectionID)
      	ToastDisplay(<§$0015§>)
      end
     type : 458752
