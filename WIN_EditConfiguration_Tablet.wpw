    name : BTN_Add
    identifier : 0x2a5f113b3cde5dff
    internal_properties : 
    properties :
     x : 309
     y : 628
     width : 255
     height : 56
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        code : |1+
         
         // Checks the name of the configuration
         if EDT_LastName = "" then
         	Error(<§$0004§>)
         
         <SI CibleExécution=Android>
         	
         		ReturnToCapture(EDT_LastName)
         	
         <SINON SI CibleExécution=iOS>
         	
         	return
         	
         <FIN>
         
         
         end
         
         // Find an existing configuration with this name
         if gnID = 0 then
         	for each Configuration 
         		if Configuration.Lastname = EDT_LastName then
         			Error(<§$0012§>)
         
         <SI CibleExécution=Android>
         	
         				ReturnToCapture(EDT_LastName)
         	
         <SINON SI CibleExécution=iOS>
         	
         	return
         	
         <FIN>
         
         			
         		end
         	end
         end
         
         // Checks the FTP server
         if EDT_ServerName = "" then
         	Error(<§$0000§>)
         
         <SI CibleExécution=Android>
         	
         		ReturnToCapture(EDT_ServerName)
         	
         <SINON SI CibleExécution=iOS>
         	
         	return
         	
         <FIN>
         
         	
         end
         
         // Check the port number
         if EDT_Port = 0 then
         	Error(<§$0001§>)
         
         <SI CibleExécution=Android>
         	
         		ReturnToCapture(EDT_Port)
         	
         <SINON SI CibleExécution=iOS>
         	
         	return
         	
         <FIN>
         
         	
         end
         
         // Retrieve the data
         ScreenToFile(WIN_EditConfiguration_Tablet,Configuration)
         
         if gnID = 0 then
         	// Add the configuration
         	HAdd(Configuration)
         	ToastDisplay(<§$0005§>)
         
         	// Enable the configuration
         	ChangeConfiguration(Configuration.ConfigurationID)
         else
         	// Modify the configuration
         	HModify(Configuration)
         	
         	LoadConfiguration()
         end
         
         Close()
        type : 18
