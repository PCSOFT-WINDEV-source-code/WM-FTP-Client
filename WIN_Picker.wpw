 menu :
  name : _Menu
  identifier : 0x2a5f112f3b232e54
  internal_properties : 
  options :
   -
     name : OPT_Supprime
     identifier : 0x2a5f112f3b242e54
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          
          
          arrID is array of int
          sPlural is string
          sNbFiles is string
          sFile is string
          bSuccessful is boolean = True
          
          for i = 1 _to_ LOOP_Content..Occurrence
          	if LOOP_Content[i].ATT_Check then
          		ArrayAdd(arrID,i)	
          	end
          end
          
          if arrID..Occurrence > 1 then
          	sPlural = "s"
          	sNbFiles = arrID..Occurrence + " "
          end
          
          
          switch Dialog(<§?1CD2D07702268645§>)
          	// &Delete
          	case 1
          		NextTitle(<§$000f§>)
          		HourGlass(True)
          		
          		for each nSubscript of arrID
          			sFile = gsCurrentLocalDir + ["/"] + LOOP_Content[nSubscript].ATT_Name
          			
          			if LOOP_Content[nSubscript].ATT_Type = ftpFile then
          				// Delete the file
          				if not fDelete(sFile) then
          					HourGlass(False)
          					Error(<§$000c§>,fExtractPath(sFile,fFileName),<§$000a§>)
          					Trace(<§$000e§>   + CR + fExtractPath(sFile,fFileName)+ CR +  ErrorInfo)
          					bSuccessful = False
          					break
          				end
          			else
          				// Delete the directory
          				if not fRemoveDir(sFile) then
          					HourGlass(False)
          					Error(<§$0011§>,sFile,<§$000a§>)
          					Trace(<§$0011§>  + CR + fExtractPath(sFile,fFileName)+ CR +  ErrorInfo)
          					bSuccessful = False
          					break
          				end
          			end
          		end
          		
          		if bSuccessful then
          			ToastDisplay(StringBuild(<§$001a§>,sPlural))		
          		end
          		
          		HourGlass(False)
          		RefreshList()
          end
         type : 32
  code_elements :
   type_code : 26
   p_codes : []
