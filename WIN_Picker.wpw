      // Summary: Callback procedure that adds a file into the looper
      //
      Procédure AddFile(sPath is string,sFile is string)
      
      // Path
      sFilePath is string = sPath + ["/"] + sFile
      
      // Calculate the size
      sSize is string = LengthToString(fSize(sFilePath))
      
      // Date of last modification
      dDate is date = fDate(sFilePath)
      tTime is time = fTime(sFilePath)
      
      // Add into the looper
      LooperAddLine(LOOP_Content,sFile, ftpFile,dDate,tTime,sSize,False,False)
      
