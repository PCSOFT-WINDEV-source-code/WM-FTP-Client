     name : MoveParentDirectoryUp
     procedure_id : 1900256461419064901
     type_code : 14
     code : |1-
      
      // Summary: Moves one level up in the tree structure
      Procédure MoveParentDirectoryUp()
      
      // move one level up
      if gsCurrentLocalDir <> "" _and_ gsCurrentLocalDir <> "/" then
      	// Calculate the parent directory
      	gsCurrentLocalDir = sDetermineLocalParentDir(gsCurrentLocalDir)
      	
      	// Change the caption
      	STC_CurrentDir = ["/"] + gsCurrentLocalDir
      	
      	// Refresh the list of files
      	RefreshList()
      	
      	// Re-display at former position
      	if gnLPRPosition <> -1 _and_ LOOP_Content..Occurrence >= gnLPRPosition then
      		LooperPosition(LOOP_Content,gnLPRPosition)
      		gnLPRPosition = -1
      	end
      	
      	result True
      else
      	result False
      end
