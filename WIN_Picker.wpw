     procedure_id : 1900256461419261509
     type_code : 14
     code : |1+
      
      // Summary: Sorts the looper by date
      //
      Procédure SortLPR(nSort is int = gnCurrentSort)
      
      gnCurrentSort = nSort
      
      switch nSort
      	case SORT_ASC_DATE
      		LooperSort("+" + "ATT_Date" + TAB + "+" + "ATT_Time")
      	case SORT_DESC_DATE
      		LooperSort("-" + "ATT_Date" + TAB + "-" + "ATT_Time")
      	case SORT_DESC_NAME
      		LooperSort("+" + "ATT_Type" + TAB + "-" + "ATT_Name")
      	case SORT_ASC_NAME
      		LooperSort("-" + "ATT_Type" + TAB + "+" + "ATT_Name")			
      	other case
      end
     type : 458752
