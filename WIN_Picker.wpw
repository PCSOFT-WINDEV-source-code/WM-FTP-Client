      Procédure EnableButton(bEnabled is boolean)
      
      if gbDownload then
      	BTN_Validate = <§$0003§>	
      else
      	BTN_Validate = <§$0004§>	
      end
      
      if bEnabled then
      	BTN_Validate..BrushColor = GlobalProcedures.COL_BLUE_1
      	BTN_Validate..State = Active
      else
      	BTN_Validate..BrushColor = GlobalProcedures.COL_GREY
