      // Summary: Refreshes the list of local files
      // Syntax:
      // RefreshList ()
      //
      Procédure RefreshList()
      
      // Manage the check of files
      gnNbSelectedFiles = 0
      
      // Disables the button
      EnableButton(False)
      OPT_Supprime..Visible = False
      
      // Delete the former records
      LooperDeleteAll(LOOP_Content)
      
      // List the folders
      fListDirectory(gsCurrentLocalDir,"WIN_Picker.AddFolder",0,frNotRecursive)
      
      // List the files
      Trace("file list")
      fListFile(gsCurrentLocalDir + "*","WIN_Picker.AddFile",0,frNotRecursive)
      
      // enables the button
      if gbDownload then
      	EnableButton(True)
      end
      
      SortLPR()
