     name : Validate
     internal_properties : CAAAAAgAAACHcy9d5duMyZuo1UJMSMutMny3dAk8pZN9I9X7QfWJyE7TRWo3jpVQjGR4zftCafaXm+GP0kPYMyojc2zlYf/OAm7ognleEJqqU72MRLRXfhhNdJSuxnMo5osDqP/BTGpI2v/Qz7nqfbvuzcYb6zltyGZa/pHkAWEcdb+Wp0gICw==
     procedure_id : 1900256461419195973
     type_code : 14
     code : |1-
      
      // Summary: Starts a download or a send operation
      //
      Procédure Validate()
      
      arrID 		is array of int
      sNbFiles is string
      sPlural	is string
      sFile 	is string
      nType 		is int
      
      if gbDownload then
      	/////////////////////////////////////////////////////////////////
      	// Download
      	// Find the files that were selected beforehand
      	for i = 1 _to_ WIN_Main.LOOP_Content..Occurrence
      		if WIN_Main.LOOP_Content[i].ATT_Check then
      			ArrayAdd(arrID,i)	
      		end
      	end
      	
      	if arrID..Occurrence > 1 then
      		sPlural = "s"
      		sNbFiles = arrID..Occurrence + " "
      	end
      	
      	switch Dialog(<§?1CD4277201990CD4§>,sPlural,sNbFiles,sPlural,gsCurrentLocalDir)
      		case 1
      			for each nSubscript of arrID
      				sFile = gsCurrentFTPDir + ["/"] + WIN_Main.LOOP_Content[nSubscript].ATT_Name
      				nType = WIN_Main.LOOP_Content[nSubscript].ATT_Type
      				
      				gclFTPCoordinator.AddTransfer(sFile,gsCurrentLocalDir,nType,True)
      			end
      			Close()	
      	end			
      else
      	/////////////////////////////////////////////////////////////////
      	// Send
      	// Finds the selected file
      	for i = 1 _to_ LOOP_Content..Occurrence
      		if LOOP_Content[i].ATT_Check then
      			ArrayAdd(arrID,i)	
      		end
      	end
      	
      	if arrID..Occurrence > 1 then
      		sPlural = "s"
      		sNbFiles = arrID..Occurrence + " "
      	end
      	
      	//1: &Send
      	//2: &Do not send
      	switch Dialog(<§?1E09A85801C31132§>,sPlural,sNbFiles,sPlural,gsCurrentFTPDir)
      		case 1
      			for each nSubscript of arrID
      				sFile = gsCurrentLocalDir + ["/"] + LOOP_Content[nSubscript].ATT_Name
      				nType = LOOP_Content[nSubscript].ATT_Type
      				gclFTPCoordinator.AddTransfer(sFile,gsCurrentFTPDir,nType,False)
      			end
      			Close()		
      	end
      end
