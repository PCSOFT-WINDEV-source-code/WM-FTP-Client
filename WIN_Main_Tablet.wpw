     procedure_id : 1900256547322166857
     type_code : 14
     code : |1-
      
      // Summary: Connection to the FTP server
      //
      Procรฉdure connection()
      
      if gbWait then
      	return
      end
      
      gbWait = True
      
      
      // Display
      STC_Wait = <ยง$0015ยง>
      GR_Wait_Home..Visible = True
      gbInConnectionError = False
      BTN_Connection..State = Inactive
      BTN_Config..State = Inactive
      Multitask(-5)
      
      // Start the animation
      StartWaitHome()
      
      // Connection in a thread to allow the progress bar to move
      Disconnect()
      Connected()
      
      // Wait for the end of connection
      while gnConnectionID = -1 _and_ gbInConnectionError = False
      	Multitask(-5)
      end
      
      if gnConnectionID <> -1 then
      	///////////////////////////////////////////////////////////////////////////////////////////
      	// Start the FTP coordinator
      	gclFTPCoordinator = new FTPCoordinator
      	
      	// We are connected
      	STC_Wait = <ยง$0016ยง>
      	
      	// Retrieving the files
      	RefreshListFTP()
      	RefreshListLocal()
      	
      	// Change the looper mode
      	ChangeLPRMode(LOOP_PENDING)
      	
      	MyWindow..Plane = 2
      end
      
      // Stop the progress bar
      BTN_Connection..State = Active
      gbWait = False
      BTN_Config..State = Active
      StopWaitHome()
