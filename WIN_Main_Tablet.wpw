      // Summary: Adds a file into the looper
      // Syntax:
      //[ <Result> = ] AddFTPFile (<sFile> is string, <nSize> is int, <sAttribute> is string, <dDate> is date, <tTime> is time)
      //
      // Parameters:
      //	sFile (ANSI string): Name of the file
      //	nSize (integer): Size of the file
      //	sAttribute (ANSI string): Attributes of the file
      //	dDate (date): Date of last modification
      //	tTime (time): <specify the role of tTime>
      
      Procรฉdure AddFTPFile(sFile is string,nSize is int, sAttribute is string, dDate is date,tTime is time)
      
      // Format the size
      sFormattedSize 	is string = LengthToString(nSize)
      bIsADirectory 	is boolean
      
      // Is it a directory
      if StringCount(sAttribute,"D",IgnoreCase) > 0 then
      	bIsADirectory = True
      end
      
      if bIsADirectory then
      	// Add the row into the looper
      	LooperAddLine(LOOP_FTP_Content,sFile,ftpDirectory,dDate,tTime,"",True)
      else
      	// Add the row into the looper
      	LooperAddLine(LOOP_FTP_Content,sFile,ftpFile,dDate,tTime,sFormattedSize,False)
      end
      
      result True
