     procedure_id : 1900256547323018825
     type_code : 14
     code : |1-
      
      // Summary: Deletes the remote files
      // Syntax:
      // DeletionFTP ()
      //
      Procรฉdure DeletionFTP()
      
      arrID is array of int
      sPlural is string
      sNbFiles is string
      sFile is string
      bSuccessful is boolean = True
      
      for i = 1 _to_ LOOP_FTP_Content..Occurrence
      	if LOOP_FTP_Content[i].ATT_Check then
      		ArrayAdd(arrID,i)	
      	end
      end
      
      if arrID..Occurrence > 1 then
      	sPlural = "s"
      	sNbFiles = arrID..Occurrence + " "
      end
      
      
      switch Dialog(<ยง?1CD2D07702268645ยง>)
      	// &Delete
      	case 1
      		NextTitle(<ยง$0006ยง>)
      		HourGlass(True)
      		
      		for each nSubscript of arrID
      			sFile = gsCurrentFTPDir + ["/"] + LOOP_FTP_Content[nSubscript].ATT_Name
      			
      			if LOOP_FTP_Content[nSubscript].ATT_Type = ftpFile then
      				// Delete the file
      				CheckConnection()
      				if not FTPDeleteFile(gnConnectionID,sFile) then
      					HourGlass(False)
      					Error(<ยง$0008ยง>,fExtractPath(sFile,fFileName),<ยง$0004ยง>)
      					Trace(<ยง$0009ยง>   + CR + fExtractPath(sFile,fFileName)+ CR +  ErrorInfo)
      					bSuccessful = False
      					break
      				end
      			else
      				// Delete the directory
      				if not FTPRemoveDir(gnConnectionID,sFile) then
      					HourGlass(False)
      					Error(<ยง$000bยง>,sFile,<ยง$0004ยง>)
      					Trace(<ยง$000bยง>  + CR + fExtractPath(sFile,fFileName)+ CR +  ErrorInfo)
      					bSuccessful = False
      					break
      				end
      			end
      		end
      		
      		if bSuccessful then
      			ToastDisplay(StringBuild(<ยง$0026ยง>,sPlural))		
      		end
      		
      		HourGlass(False)
      		
      		EnableDownloadButton(False)
      		RefreshListFTP()
      end
     type : 458752
