     name : ChangeLPRMode
     internal_properties : CAAAAAgAAACLeamw41kbYAfPcVtnnvqjE3zJOG/0fEsoihi34xhcII/A3BChEuPiseuSXqRw4Y5cbgaKBT1Ng+Te3eMlg+EovZK8KpAkA1QDa8CQJ8Bh2YcqJ2jKMZsZbHiiUycew8ukidALXkOeSQTgd9sTQ8uP1RkF67O66B/DIVkuvCZ/V+9wQEJ9H37U8cZWP2KymS15XnGR7iOs8kxyD8iSgbYLWYvbA8nJ5fbqKWdifBBkQf/UeA==
     procedure_id : 1900256547323149897
     type_code : 14
     code : |1+
      // Summary: Changes the looper mode 
      // Syntax:
      //ChangeLPRMode (<nMode> is int)
      //
      // Parameters:
      //	nMode (integer): <specify the role of nMode>
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procรฉdure ChangeLPRMode(nMode is int)
      
      // Arrays of transfers sorted by type
      arrPendingTransfers 		is array of FTPTransfer
      arrCurrentTransfers		is array of FTPTransfer
      arrCompletedTransfers		is array of FTPTransfer
      arrTransfersInError		is array of FTPTransfer
      arrCanceledTransfers		is array of FTPTransfer
      gnLPRMode = nMode
      
      // Tabs
      SC_Transfers.BTN_LOOP_Pending..State = Active
      SC_Transfers.BTN_LOOP_Done..State = Active
      SC_Transfers.BTN_LPR_InError..State = Active
      
      switch gnLPRMode
      	case LOOP_PENDING
      		SC_Transfers.LOOP_Transfers..PopupMenu = MENU_Transfer
      		SC_Transfers.BTN_LOOP_Pending..State = Grayed
      	case LOOP_DONE
      		SC_Transfers.LOOP_Transfers..PopupMenu = MENU_Done
      		SC_Transfers.BTN_LOOP_Done..State = Grayed
      	case LOOP_ERROR
      		SC_Transfers.LOOP_Transfers..PopupMenu = MENU_InError
      		SC_Transfers.BTN_LPR_InError..State = Grayed
      	other case
      		
      end
      
      
      // Refresh the looper
      if gclUIManager.bInFunction then
      	gclUIManager.Refresh()
      else
      		// Sort the transfers
      		if gclFTPCoordinator <> Null then
      			for each clTransfer of gclFTPCoordinator.m_arrTransfers
      				switch clTransfer.m_nStatus
      					case FTPTransfer.STATUS_CANCELED
      						ArrayAdd(arrCanceledTransfers,clTransfer)
      					case FTPTransfer.STATUT_PENDING
      						ArrayAdd(arrPendingTransfers,clTransfer)
      					case FTPTransfer.STATUS_INPROGRESS
      						ArrayAdd(arrCurrentTransfers,clTransfer)
      					case FTPTransfer.STATUS_ERROR
      						ArrayAdd(arrTransfersInError,clTransfer)
      					case FTPTransfer.STATUS_COMPLETED
      						ArrayAdd(arrCompletedTransfers,clTransfer)
      					other case
      				end
      			end
      		end
      		
      		// Current transfer and counters
      		if arrCurrentTransfers..Occurrence = 0 then
      			GR_CurrentTransfer..Visible = False	
      		end
      		SC_Transfers.STC_Pending = arrPendingTransfers..Occurrence + arrCurrentTransfers..Occurrence
      		SC_Transfers.STC_Done = arrCompletedTransfers..Occurrence
      		SC_Transfers.STC_InError = arrTransfersInError..Occurrence
      	
      		// Refreshes the UI
      		gclUIManager.RefreshTabletLPR(arrPendingTransfers,arrCurrentTransfers,arrCompletedTransfers,arrTransfersInError)
      end
