     name : RefreshConfig
     procedure_id : 1900256547323346505
     type_code : 14
     code : |1-
      // Summary: Refreshes the configuration
      //
      Procรฉdure RefreshConfig(bCloseMenu is boolean)
      
      IMG_BurgerIcon..Visible = True
      MyWindow..Plane = 1
      
      // Refresh the list of local files
      STC_CurrentDir_Local = ["/"] + gstConfiguration.sLocalDirectory
      STC_CurrentDir_FTP = ["/"] + gstConfiguration.sFTPDirectory
      if gnConnectionID <> -1 then
      	RefreshListLocal()
      end
      
      if bCloseMenu then
      	WinSlidingVisible(swLeft,False)	
      end
      
      // If we want to connect and refresh the display
      if gbConnectAndRefresh then
      	gbConnectAndRefresh = False
      	
      	// Display
      	BTN_Connection..State = Active
      	MyWindow..Title = gstConfiguration.sName
      	BTN_Config..Caption = gstConfiguration.sName
      	
      	// Connection to FTP server
      	Disconnect()
      	MyWindow..Plane = 1
      	IMG_BurgerIcon..Visible = True
      	connection()
      end
      
      // If no configuration was created
      if HNbRec(Configuration) = 0 then
      	BTN_Connection..State = Grayed
      	IMG_BurgerIcon..Visible = False
      	MyWindow..Plane = 3
      else
      	// If no configuration is selected
      	if gnConfigurationID < 1 then
      		HReadFirst(Configuration)
      		if not HOut(Configuration) then
      			ChangeConfiguration(Configuration.ConfigurationID)
      		end
      	end
      	//////////////////////////////////////////////////////////
      	// Configuration loaded
      	BTN_Connection..State = Active
      	MyWindow..Title = gstConfiguration.sName
      	BTN_Config..Caption = gstConfiguration.sName
      	if gnConnectionID <> -1 then
      		// We are connected
      		ChangeLPRMode(LOOP_PENDING)
      		// Change the plane
      		MyWindow..Plane = 2
      	end
      end
