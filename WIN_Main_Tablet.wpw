      // Summary: Refreshes the list of FTP files
      // Syntax:
      //RefreshListTP ([<bPullToRefresh> is boolean])
      //
      Procรฉdure RefreshListFTP(bPullToRefresh is boolean = False)
      
      gbWait = True
      sDir is string
      
      // Modify the available rights
      MENU_FTP.OPT_Access_rights_to_the_file..Visible = gbCTPLAvailable _and_ not IniOSMode()
      
      if gsCurrentFTPDir = "" then
      	sDir = "/"
      else
      	sDir = ["/"] + gsCurrentFTPDir
      end
      
      // Display
      StartRefresh(bPullToRefresh)
      Multitask(-5)
      LOOP_FTP_Content..Visible = False
      
      // Manage the selection of elements
      gnNbSelectedFTPFiles = 0
      EnableDownloadButton(False)
      
      // Delete the former records
      LooperDeleteAll(LOOP_FTP_Content)
      
      Trace("Refreshes FTP List")
      ChronoStart(70)
      
      // List the folders
      CheckConnection()
      FTPListFile(gnConnectionID,sDir,"WIN_Main_Tablet.AddFTPFile",ftpDirectory +ftpFile)
      
      // Sort the looper
      SortFTPLPR()
      
      // Display
      LOOP_FTP_Content..Visible = True
      gbWait = False
      StopRefresh(bPullToRefresh)
      
      Trace("Refreshes FTP List - Ended in " + ChronoEnd(70))
