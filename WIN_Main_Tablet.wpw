      // Summary: Sorts the FTP looper
      //
      Procรฉdure SortFTPLPR(nSort is int = gnCurrentFTPSort)
      
      gnCurrentFTPSort = nSort
      
      switch nSort
      	case SORT_ASC_DATE
      		LooperSort("+" + "LOOP_FTP_Content.ATT_Date" + TAB + "+" + "ATT_Time")
      	case SORT_DESC_DATE
      		LooperSort("-" + "LOOP_FTP_Content.ATT_Date" + TAB + "-" + "ATT_Time")
      	case SORT_DESC_NAME
      		LooperSort("+" + "LOOP_FTP_Content.ATT_Type" + TAB + "-" + "ATT_Name")
      	case SORT_ASC_NAME
      		LooperSort("-" + "LOOP_FTP_Content.ATT_Type" + TAB + "+" + "ATT_Name")
      	other case
      end
