      	if gnPositionFTPLPR <> -1 _and_ LOOP_FTP_Content..Occurrence >= gnPositionFTPLPR then
      		LooperPosition(LOOP_FTP_Content,gnPositionFTPLPR)
      		gnPositionFTPLPR = -1
      	end
      	
      	result True
      else
      	result False
      end
