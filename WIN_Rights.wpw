     code : |1-
      // Summary: Modify the rights
      // Syntax:
      //[ <Result> = ] ModifyRights (<sFilePath> is string, <sValue> is string)
      //
      // Parameters:
      //	sFilePath (ANSI string): <specify the role of sFilePath>
      //	sValue (ANSI string): <specify the role of EDT_Value>
      // Return value:
      // 	boolean: // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procédure ModifyRights(sFilePath is string,sValue is string)
      
      sAnswer is string
      sCommand is string = "CHMOD SITE " + sValue + sFilePath
      
      // Run the command
      result FTPCommand(gnConnectionID, sCommand, sAnswer) 
     type : 458752
   -
     name : AskToModifyRights
     procedure_id : 1900256354039615121
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      // AskToModifyRights ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procédure AskToModifyRights()
      
      NextTitle(<§$0004§>)
      HourGlass(True)
      
      CalculateNumericValue()
      
      // Start the modification
      ListFTPFiles(gsFilePath,0,"D",0,0,EDT_Value)
      
      HourGlass(False)
      
      if not gbError then
      	ToastDisplay(<§$0005§>)
      	Close()
      else
      	Error(<§$0006§>,ErrorInfo())
      end
     type : 458752
