     name : CalculateNumericValue
     procedure_id : 1900256354039680657
     type_code : 14
     code : |1-
      // Summary: Converts the check boxes into numeric value
      //
      Procédure CalculateNumericValue()
      
      nVal1,nVal2,nVal3 are int
      
      if CBOX_User_Rights[1] then
      	nVal1 += 4
      end
      if CBOX_User_Rights[2] then
      	nVal1 += 2
      end
      if CBOX_User_Rights[3] then
      	nVal1 += 1
      end
      
      if CBOX_Group_Rights[1] then
      	nVal2 += 4
      end
      if CBOX_Group_Rights[2] then
      	nVal2 += 2
      end
      if CBOX_Group_Rights[3] then
      	nVal2 += 1
      end
      
      if CBOX_Other_Rights[1] then
      	nVal3 += 4
      end
      if CBOX_Other_Rights[2] then
      	nVal3 += 2
      end
      if CBOX_Other_Rights[3] then
      	nVal3 += 1
      end
      
      EDT_Value = nVal1 + "" + nVal2 + "" + nVal3
     type : 458752
