     name : ListFTPFiles
     internal_properties : CAAAAAgAAACunwF5F3JssS4Cz7jPSUvuaDHW1cr3mTw1G1qUkAGQd7bD8fE3u/GkabZreVhw1insOwVgOmQtjNsLmGILqpUIjWuPOFyyW25rZKoVR9ojqE/Mh6pBlGM4epSBIt+yM4InpB6lBstqMV6iihF1
     procedure_id : 1900256354039811729
     type_code : 14
     code : |1+
      
      // Summary: Callback for ftplistfile
      // Syntax:
      //[ <Result> = ] ListFTPFile (<sFile> is string, <nSize> is int, <sAttribute> is string, <dDate> is date, <tTime> is time, <sRights> is string)
      //
      // Parameters:
      //	sFile (ANSI string): Name of the file
      //	nSize (integer): Size of the file
      //	sAttribute (ANSI string): Attributes of the file
      //	dDate (date): Date of last modification
      //	tTime (time): <specify the role of tTime>
      //	sRights (ANSI string): <specify the role of sRights>
      
      Procédure ListFTPFiles(sFile is string,nSize is int, sAttribute is string, dDate is date <useful>,tTime is time <useful>,local sRights is string )
      
      // Format the size
      sFormattedSize 	is string = LengthToString(nSize)
      bIsADirectory 	is boolean
      
      // Is it a directory
      if StringCount(sAttribute,"D",IgnoreCase) > 0 then
      	bIsADirectory = True
      end
      
      // Modifying the rights
      if (sFile = gsFilePath) _or_ (CBOX_Recursion_Folders _and_ bIsADirectory) _or_ (CBOX_Recursion_Files _and_ not bIsADirectory) then
      	ModifyRights(sFile,sRights)
      	gbError = True
      	result False
      end
      
      // Recursivity
      if bIsADirectory _and_ CBOX_Recursion = True then
      	// We want a recursive modification therefore the files must be listed
      	FTPListFile(gnConnectionID,gsFilePath,"WIN_Rights.ListFTPFiles",ftpDirectory +ftpFile,sRights)
      end
     type : 458752
